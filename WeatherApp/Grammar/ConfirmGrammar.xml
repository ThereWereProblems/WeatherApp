<grammar version="1.0" xml:lang="pl-pl" root="rootRule" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">

	<rule id="rootRule">
		<ruleref uri="#First"></ruleref>
		<tag>out.First=rules.latest();</tag>
	</rule>

	<rule id="First">
		<one-of>
			<item>
				Tak<tag>out="Tak"</tag>
			</item>
			<item>
				Potwierdzam<tag>out="Tak"</tag>
			</item>
			<item>
				Okej<tag>out="Tak"</tag>
			</item>
			<item>
				Nie<tag>out="Nie"</tag>
			</item>
			<item>
				Odmawiam<tag>out="Nie"</tag>
			</item>
			<item>
				Nie potwierdzam<tag>out="Nie"</tag>
			</item>
		</one-of>
	</rule>

</grammar>
